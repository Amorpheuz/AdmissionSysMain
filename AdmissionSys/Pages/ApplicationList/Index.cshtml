@page
@model AdmissionSys.Pages.ApplicationList.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

@*<p>
    <a asp-page="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
            <th>
                Program Name
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationList[0].ApplicationCat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationList[0].Status)
            </th>

           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ApplicationList) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Programs.ProgramName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationCat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>


            <td>
                <a asp-page="./Delete" asp-route-id="@item.ApplicationListID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
